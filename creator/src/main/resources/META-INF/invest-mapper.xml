<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="InvestMapper">
	
	<select id="investList" resultType="kr.co.creator.vo.ProjectVO">
		select project_name, 
				grade, 
				yield, 
				refund, 
				price, 
				repay_method, 
				ach_rate, 
				ach_state 
		from project_list
	</select>
	
</mapper>