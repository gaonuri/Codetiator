<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="SupportMapper">
	
	<insert id="supportInsert"
			parameterType="kr.co.creator.vo.NoticeVO">
			insert into notice( title
							, notice_contents
							, notice_date)
		values( #{title}
				, #{notice_contents}
				, now())
	</insert>
	
	<select id="supportList"
			resultType="kr.co.creator.vo.NoticeVO">
		select notice_num
			, title
			, notice_date
			FROM notice
			order by notice_date desc
	</select>
	
	<select id="supportList2"
			resultType="kr.co.creator.vo.NoticeVO">
		select notice_num
			, title
			, notice_date
			FROM notice
			order by notice_date desc
	</select>
	
	<select id="supportList3"
			resultType="kr.co.creator.vo.NoticeVO">
		select notice_num
			, title
			, notice_date
			FROM notice
			order by notice_date desc
	</select>
	
